apiVersion: route.openshift.io/v1
kind: Route
metadata:
  name: {{ catalog_service_name }}
spec:
  port:
    targetPort: http
  to:
    kind: Service
    name: {{ catalog_service_name }}
    weight: 100
  wildcardPolicy: None
  tls:
    insecureEdgeTerminationPolicy: Redirect
    termination: edge

