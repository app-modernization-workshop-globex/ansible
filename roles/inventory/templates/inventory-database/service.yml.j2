kind: Service
metadata:
  labels:
    app.kubernetes.io/name: {{ inventory_db_name }}
    app.kubernetes.io/part-of: {{ inventory_app_name }}
  name: {{ inventory_db_name }}
spec:
  ports:
    - name: postgresql
      protocol: TCP
      port: 5432
      targetPort: 5432
  selector:
    app.kubernetes.io/name: {{ inventory_db_name }}